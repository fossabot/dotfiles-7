# -------- SYSTEM ---------
alias can_has='sudo apt-get install'
alias ll='ls -la'
alias l='ls -lah'

# -------- SSH ---------
alias htdev='ssh cs@entwicklung.hightext.de'
alias cmsp='ssh root@cmsp.hightext.de -i ~/.ssh/hightext_cmsp'

# -------- GIT ---------
alias gs='git status'
alias ga='git add .'
